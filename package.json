{
  "author": "Kasper Tidemann",
  "bin": "./bin/js/cli.js",
  "bugs": {
    "url": "https://github.com/tdmnco/elie/issues"
  },
  "description": "Elie is eloquent writing based on simple Markdown processing.",
  "dependencies": {
    "glob": "7.1.4",
    "gray-matter": "4.0.2",
    "html-minifier": "4.0.0",
    "marked": "0.7.0",
    "mkdirp": "0.5.1",
    "pretty-bytes": "5.3.0",
    "slugify": "1.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "babel-eslint": "10.0.2",
    "eslint": "6.1.0",
    "jest": "24.8.0",
    "npm": "6.10.2"
  },
  "homepage": "https://github.com/tdmnco/elie#readme",
  "keywords": [
    "javascript",
    "json",
    "tdmnco",
    "writing"
  ],
  "license": "MIT",
  "main": "src/js/elie.js",
  "name": "elie",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdmnco/elie.git"
  },
  "scripts": {
    "test:build": "node bin/js/cli.js --input=test/markdown --header=test/html/header.html --footer=test/html/footer.html --output=build",
    "test:build:index": "node bin/js/cli.js --input=test/markdown/index.md --header=test/html/header.html --footer=test/html/footer.html --output=build",
    "test:build-minify": "node bin/js/cli.js --input=test/markdown --header=test/html/header.html --footer=test/html/footer.html --output=build --minify=true",
    "test:build-minify:index": "node bin/js/cli.js --input=test/markdown/index.md --header=test/html/header.html --footer=test/html/footer.html --output=build --minify=true"
  },
  "version": "0.1.0"
}
