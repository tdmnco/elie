{
  "author": "Kasper Tidemann",
  "bin": "./bin/js/cli.js",
  "bugs": {
    "url": "https://github.com/tdmnco/elie/issues"
  },
  "description": "Elie is eloquent writing based on simple Markdown processing.",
  "dependencies": {
    "chalk": "2.4.2",
    "glob": "7.1.4",
    "gray-matter": "4.0.2",
    "marked": "0.7.0",
    "mkdirp": "0.5.1",
    "pretty-bytes": "5.3.0",
    "prettier": "1.18.2",
    "slugify": "1.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.4.0",
    "jest": "24.9.0",
    "npm": "6.14.6"
  },
  "homepage": "https://github.com/tdmnco/elie#readme",
  "keywords": [
    "blog",
    "javascript",
    "json",
    "markdown",
    "tdmnco",
    "writing"
  ],
  "license": "MIT",
  "main": "src/js/elie.js",
  "name": "elie",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdmnco/elie.git"
  },
  "scripts": {
    "test:build": "node bin/js/cli.js --input=test/markdown --output=build --templates=test/templates",
    "test:build:index": "node bin/js/cli.js --input=test/markdown/index.md --output=build --templates=test/templates",
    "test:build:posts": "node bin/js/cli.js --input=test/markdown/posts --output=build --templates=test/templates"
  },
  "version": "0.2.3"
}
